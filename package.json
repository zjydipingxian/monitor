{
  "name": "monitor",
  "version": "1.0.0",
  "description": "前端稳定性监控",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "inits": "pnpm install",
    "clean": "pnpm -r --filter=./packages/* run clean",
    "clearDist": "pnpm -r --filter=./packages/* run clearDist",
    "build": "pnpm -r --filter=./packages/* run clearDist && node scripts/build.mjs",
    "pub": "pnpm run build && pnpm -r --filter=./packages/* publish",
    "pub:beta": "pnpm run build && pnpm -r --filter=./packages/* publish --tag beta"
  },
  "keywords": [
    "encode",
    "monitor"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.10",
    "execa": "^5.1.1",
    "rollup": "^4.18.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "rimraf": "^6.0.1"
  }
}
