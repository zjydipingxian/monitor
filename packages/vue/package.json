    "encode",
    "monitor",
    "Vue 监控"
  ],
  "buildOptions": {
    "name": "monitorVue",
    "formats": [
      "cjs",
      "global"
    ]
  },
  "author": "ZhongJiaYao",
  "license": "ISC",
  "dependencies": {
    "monitor-core": "workspace:^",
    "monitor-shared": "workspace:^",
    "monitor-types": "workspace:^",
    "monitor-utils": "workspace:^"
  }
}
